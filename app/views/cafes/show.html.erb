<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @cafe.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @cafe.description %>
</p>

<p>
  <strong>Address:</strong>
  <%= @cafe.address %>
</p>

<p>
  <strong>Longitude:</strong>
  <%= @cafe.longitude %>
</p>

<p>
  <strong>Latitude:</strong>
  <%= @cafe.latitude %>
</p>

<p>
  <strong>Hours of Operation:</strong>
  <%= @cafe.latitude %>
</p>

<p>
  <strong>Rating:</strong>
  <%= @cafe.rating %>
</p>

<%= render 'reviews/review' %>

<% if logged_in? %>
  <%= render 'reviews/form' %>
  <% if @cafe && @cafe.user_id == current_user.id %>
    <%= link_to 'Edit', edit_cafe_path(@cafe) %> |
  <% else %>
  <% end %>
<% end %>
